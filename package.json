{
    "name": "jov",
    "version": "0.0.3",
    "description": "JavaScript Object Validator",
    "main": "index.js",
    "scripts": {
        "build": "babel src --presets babel-preset-es2015 --out-dir dist",
        "eslint": "eslint .; exit 0",
        "jest": "jest",
        "coverage": "jest --coverage",
        "test": "jest",
        "posttest": "npm run eslint",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oenstrom/jov.git"
    },
    "keywords": [
        "javascript",
        "objects",
        "validation"
    ],
    "author": "Olof Enstr√∂m",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/oenstrom/jov/issues"
    },
    "homepage": "https://github.com/oenstrom/jov#readme",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^4.12.1",
        "jest": "^21.2.1"
    },
    "jest": {
        "verbose": true,
        "testMatch": [
            "**/test/**/*.test.js"
        ],
        "collectCoverageFrom": [
            "lib/**/*.js"
        ]
    }
}
